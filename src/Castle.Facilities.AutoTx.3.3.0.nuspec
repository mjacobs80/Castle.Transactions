    <copyright>Copyright 2004-2012 Castle Project - http://www.castleproject.org/</copyright>
    <tags>Castle NHibernate</tags>
    <dependencies>
      <dependency id="Castle.Core" version="3.3.0" />
      <dependency id="Castle.Windsor" version="3.3.0" />
      <dependency id="Castle.Services.Transaction" version="[3.3.0]" />
    </dependencies>
  </metadata>
  <files>
	<file src="Castle.Facilities.AutoTx\build-output\*.dll" target="lib\net40" />
	<file src="Castle.Facilities.AutoTx\build-output\*.xml" target="lib\net40" />
	<file src="Castle.Facilities.AutoTx\build-output\*.pdb" target="lib\net40" />
	<file src="Castle.Facilities.AutoTx\src\**\*.cs" target="src" />
  </files>
</package>